{
    "name": "fico7489/undabot",
    "authors": [
        {
            "name": "Filip Horvat",
            "email": "fico7489@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "7.*",
        "symfony/runtime": "7.*",
        "symfony/yaml": "7.*",
        "symfony/flex": "2.4.*",
        "symfony/dotenv": "7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "10.*",
        "friendsofphp/php-cs-fixer": "3.*",
        "phpstan/phpstan": "1.*"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
